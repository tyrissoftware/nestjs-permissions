{
  "name": "@tyristv/nestjs-permissions",
  "version": "1.0.0",
  "description": "Extension library for ACL in NestJS",
  "homepage": "https://github.com/tyrissoftware/nestjs-permissions",
  "files": [
    "dist"
  ],
  "scripts": {
